{
  "name": "www",
  "version": 2,
  "alias": "www",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next",
      "config": {
        "useBuildUtils": "@now/build-utils@canary",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/(.*)",
      "headers": {"cache-control": "max-age=31536000"},
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*)",
      "headers": {"cache-control": "max-age=600"},
      "dest": "/$1"
    }
  ],
  "env": {
    "WWW_PREFIX": "",
    "API_HOST": "edu-1.herokuapp.com/v1alpha1/graphql",
  }
}
