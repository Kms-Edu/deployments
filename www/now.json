{
  "alias": "www",
  "name": "www",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next",
      "config": {
        "useBuildUtils": "@now/build-utils@canary",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/(.*)",
      "headers": {"cache-control": "max-age=86400"},
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
